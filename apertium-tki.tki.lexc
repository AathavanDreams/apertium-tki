%{q%}		      ! for loanwords, devoicing suffixes
%{ğ%}                 ! ^^ opposite
%{e%}                 ! forces front harmony
%{a%}                 ! forces back harmony
%{L%}                 ! for passive: ل or ن
%{l%}                 ! for l deletion 
%{y%}                 ! realised as ي 



